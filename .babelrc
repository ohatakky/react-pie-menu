{
  "presets": [
    ["env", {
      "modules": false
    }],
    "stage-0",
    "react"
  ],
  "plugins": [
    "transform-decorators-legacy",
    ["lodash", {"id": ["lodash", "recompose"]}],
    ["module-resolver", { 
      "root": ["./"],
      "cwd": "./",
      "alias": {
        "^react-pie-menu$": "./"
      }
    }],
    "babel-plugin-styled-components"
  ],
  "env": {
    "commonjs": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}